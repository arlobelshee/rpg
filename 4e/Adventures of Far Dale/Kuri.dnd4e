            <specific name="Level" > 2 </specific>
            <specific name="Power Type" > Utility </specific>
            <specific name="_Tags" > Essentials </specific>
            <specific name="_ParentFeature" > ID_FMP_CLASS_FEATURE_3041 </specific>
            <specific name="_Subclasses" > ID_FMP_CLASS_722 </specific>
