         <RulesElement name="Natural Jumper" type="Racial Trait" internal-id="ID_FMP_RACIAL_TRAIT_2491" charelem="56" legality="rules-legal" >
            <specific name="Short Description" > You are always considered to have a running start when jumping </specific>
            <specific name="Level" > 1 </specific>
            <specific name="Class" > ID_FMP_RACE_51 </specific>
