         </RulesElement>
         <RulesElement name="Low-light" type="Vision" internal-id="ID_INTERNAL_VISION_LOW-LIGHT" charelem="60" legality="rules-legal" >
         </RulesElement>
         <RulesElement name="Multiple Arms" type="Racial Trait" internal-id="ID_FMP_RACIAL_TRAIT_2490" charelem="61" legality="rules-legal" >
            <specific name="Short Description" > Draw or sheathe a weapon as free action 1/turn </specific>
            <specific name="Level" > 1 </specific>
            <specific name="Class" > ID_FMP_RACE_51 </specific>
