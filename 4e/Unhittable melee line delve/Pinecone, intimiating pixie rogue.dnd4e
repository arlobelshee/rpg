            <specific name="_SupportsID" > ID_FMP_CLASS_6 </specific>
         </RulesElement>
         <RulesElement name="Disheartening Strike" type="Power" internal-id="ID_FMP_POWER_2248" url="http://www.wizards.com/dndinsider/compendium/power.aspx?id=2248" charelem="79" legality="rules-legal" >
            <specific name="Flavor" > The bite of your weapon is deepened by the sting of your ire. </specific>
            <specific name="Power Usage" > At-Will </specific>
            <specific name="Display" > Rogue Attack 1 </specific>
            <specific name="Keywords" > Martial, Rattling, Weapon </specific>
            <specific name="Action Type" > Standard Action </specific>
            <specific name="Attack Type" > Melee or Ranged weapon </specific>
