         <RulesElement name="Warlord" type="Class" internal-id="ID_FMP_CLASS_8" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=8" charelem="18" legality="rules-legal" >
            <specific name="Key Abilities" > Strength; Intelligence or Charisma </specific>
