         <RulesElement name="Living Construct" type="Racial Trait" internal-id="ID_FMP_RACIAL_TRAIT_406" charelem="68" legality="rules-legal" >
            <specific name="Short Description" > No need to eat, drink, breathe, or sleep </specific>
