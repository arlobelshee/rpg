         <RulesElement name="Storm&apos;s Embrace" type="Class Feature" internal-id="ID_FMP_CLASS_FEATURE_1563" charelem="119" legality="rules-legal" >
            <specific name="Short Description" > On a 20 push 1 and fly 1+ your Dexterity modifier. </specific>
            <specific name="Level" > 1 </specific>
