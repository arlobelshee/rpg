         <RulesElement name="Fighter" type="Class" internal-id="ID_FMP_CLASS_3" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=3" charelem="19" legality="rules-legal" >
            <specific name="Key Abilities" > Strength; Dexterity, Wisdom, or Constitution </specific>
