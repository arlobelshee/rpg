         <RulesElement name="Elementalist" type="Class" internal-id="ID_FMP_CLASS_956" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=956" charelem="6" legality="rules-legal" >
            <specific name="Key Abilities" > Charisma, Constitution </specific>
            <specific name="Implements" > Daggers, staffs </specific>
            <specific name="Armor Proficiencies" > Cloth </specific>
            <specific name="Weapon Proficiencies" > Simple melee, simple ranged </specific>
