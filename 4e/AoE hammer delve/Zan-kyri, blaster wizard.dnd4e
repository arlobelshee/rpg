         <RulesElement name="Mage" type="Class" internal-id="ID_FMP_CLASS_722" url="http://www.wizards.com/dndinsider/compendium/class.aspx?id=722" charelem="6" legality="rules-legal" >
            <specific name="Key Abilities" > Intelligence; Constitution, Wisdom, or Charisma </specific>
